   <div id="header">
    <% if signed_in? -%>
      <%= button_to 'Sign Out', sign_out_path, method: :delete %>
    <% else -%>
      <%= button_to 'Sign In', sign_in_path %>
    <% end -%>
  </div>

  <div id="flash">
    <% flash.each do |key, value| -%>
      <div id="flash_<%= key %>"><%=h value %></div>
    <% end %>
  </div>
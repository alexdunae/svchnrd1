<%= form_for(@user) do |f| %>

  title<br>
  <%= f.text_field :title %>

  first name<br>
  <%= f.text_field :first_name %>

  last name<br>
  <%= f.text_field :last_name %>

  user name<br>
  <%= f.text_field :user_name %>

  <br><br>
  email<br>
  <%= f.email_field(:email, {}) %>

  <br><br>
  password<br>
  <%= f.password_field :password %>

  <button type="submit">Save</button>

<% end %>


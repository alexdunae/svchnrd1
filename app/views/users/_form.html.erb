<%= form_for(@user) do |f| %>

  <div>
  title<br>
  <%= f.text_field :title %></div>

  <div>
  first name<br>
  <%= f.text_field :first_name %></div>
  
  <div>
  last name<br>
  <%= f.text_field :last_name %></div>
  
  <div>
  user name<br>
  <%= f.text_field :user_name %></div>
  
  <div>
  email<br>
  <%= f.email_field(:email, {}) %></div>

  <div>
  password<br>
  <%= f.password_field :password %>
  </div>

  <button type="submit">Save</button>

<% end %>

